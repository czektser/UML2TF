<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://www.eclipse.org/emf/2002/Ecore
                                    http://www.eclipse.org/emf/2002/Ecore/Ecore.xsd"
                name="myModel"
                nsURI="http://www.example.org/myModel"
                nsPrefix="myModel">
  <eClassifiers xsi:type="ecore:EClass" name="Person">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="age" eType="ecore:EInt"/>
  </eClassifiers>
  <eClasses xsi:type="myModel:Person" name="John Doe" age="30"/>
</ecore:EPackage>